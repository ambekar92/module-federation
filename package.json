{
   "name": "ucms-wfe-app",
   "version": "0.1.0",
   "private": true,
   "workspaces": [
      "./admin",
      "./home",
      "./entity_application",
      "./evaluation",
      "./field_operation",
      "./helpdesk",
      "./user"
   ],
   "scripts": {
      "dev": "rimraf .next && cross-env NEXT_PRIVATE_LOCAL_WEBPACK=true next dev -p 8080",
      "start": "concurrently \"npm --prefix admin run dev\" \"npm --prefix home run dev\" \"npm --prefix entity_application run dev\" \"npm --prefix evaluation run dev\" \"npm --prefix field_operation run dev\" \"npm --prefix helpdesk run dev\" \"npm --prefix user run dev\"",
      "build": "concurrently \"npm --prefix admin run build\" \"npm --prefix home run build\" \"npm --prefix entity_application run build\" \"npm --prefix evaluation run build\" \"npm --prefix field_operation run build\" \"npm --prefix helpdesk run build\" \"npm --prefix user run build\"",
      "serve": "concurrently \"npm --prefix admin run start\" \"npm --prefix home run start\" \"npm --prefix entity_application run start\" \"npm --prefix evaluation run start\" \"npm --prefix field_operation run start\" \"npm --prefix helpdesk run start\" \"npm --prefix user run start\"",
      "e2e:ci": "pnpm run build",
      "lint": "eslint --fix",
      "pretty": "prettier --write .",
      "prepare": "husky install"
   },
   "dependencies": {
      "@emotion/react": "^11.11.3",
      "@emotion/styled": "^11.11.0",
      "@fortawesome/fontawesome-svg-core": "^6.5.1",
      "@fortawesome/free-solid-svg-icons": "^6.5.1",
      "@fortawesome/react-fontawesome": "^0.2.0",
      "@module-federation/nextjs-mf": "^8.1.10",
      "@mui/material": "^5.15.9",
      "@okta/okta-auth-js": "^7.5.1",
      "@trussworks/react-uswds": "^7.0.0",
      "@uswds/uswds": "3.7.1",
      "classnames": "^2.5.1",
      "commander": "^12.0.0",
      "concurrently": "^8.2.2",
      "n": "^9.2.1",
      "next": "13.5.6",
      "next-auth": "^4.24.6",
      "react": "^18.2.0",
      "react-bootstrap": "^2.10.1",
      "react-dom": "^18.2.0",
      "react-helmet": "^6.1.0",
      "react-router-dom": "^6.22.2",
      "react-toastify": "^10.0.5",
      "sass": "^1.71.1",
      "sass-loader": "^14.1.1",
      "webpack": "^5.90.3"
   },
   "devDependencies": {
      "@axe-core/react": "^4.8.4",
      "@faker-js/faker": "^8.4.1",
      "@react-docgen/cli": "^2.0.3",
      "@types/node": "^20",
      "@types/react": "^18",
      "@types/react-dom": "^18",
      "@types/react-helmet": "^6.1.11",
      "@typescript-eslint/eslint-plugin": "^6.21.0",
      "cross-env": "^7.0.3",
      "eslint": "^8.57.0",
      "eslint-config-next": "13.5.6",
      "eslint-config-prettier": "^9.1.0",
      "eslint-config-standard-with-typescript": "^43.0.1",
      "eslint-plugin-import": "^2.29.1",
      "eslint-plugin-n": "^16.6.2",
      "eslint-plugin-prettier": "^5.1.3",
      "eslint-plugin-promise": "^6.1.1",
      "eslint-plugin-react": "^7.33.2",
      "husky": "^8.0.0",
      "lint-staged": "^15.2.2",
      "prettier": "^3.2.5",
      "style-loader": "^3.3.4",
      "typescript": "^5.3.3"
   },
   "lint-staged": {
      "**/*.{js,jsx,ts,tsx}": [
         "npm run lint",
         "prettier --write"
      ]
   }
}
